      "default": "./dist/clerkSWR.js"
    },
    "./getInnerSchema": {
      "default": "./dist/getInnerSchema.js"
    },
    "./textHelpers": {
      "default": "./dist/textHelpers.js"
    },
    "./arrayHelpers": {
      "default": "./dist/arrayHelpers.js"
    },
    "./OrgContext": {
      "default": "./src/hooks/OrgContext.tsx"
    },
