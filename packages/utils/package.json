    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "little-state-machine": "^5.0.1",
    "power-set": "^1.1.8",
    "react": "^19.0.0",
    "react-autosave": "^0.5.0",
    "react-dom": "^19.0.0",
    "swr": "^2.3.3",
    "zod": "^3.24.2"
